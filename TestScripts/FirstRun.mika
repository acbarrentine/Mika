GlobalVar : int <- 12

fun TestEntry() : int
{
	var1 : int
	var1 <- 4 + Twice(GlobalVar)
	AssertEqualsInt(var1, 28)

	if Twice(3) >= 6
		Assert(1)
	else
	{
		Assert(0)
	}

	var1 <- 1
	while (var1 < 10)
	{
		var1 <- var1 + 1
	}
	AssertEqualsInt(var1, 10)

	AssertEqualsInt(AnotherGlobalVar+1, 21)
	return AnotherGlobalVar
}

AnotherGlobalVar : int <- 20

fun Twice(val:int) : int
{
	return 2 * val
}
